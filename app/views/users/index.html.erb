<%= render '/users/userinfo', user: current_user %>

<%= render '/books/new_book', book: @book %>

<div>
  <table>
    <thead>
      <tr>
        <th>image</th>
        <th>name</th>
      </tr>
    </thead>
    <tbody>
      <% @users.each do |user| %>
        <tr>
          <td><%= image_tag user.get_profile_image(100,100) %></td>
          <td><%= user.name %></td>
          <td><%= link_to 'Show', user_path(user.id) %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>